    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= current_page.data.title || "Kwab Boateng" %></title>
    <%= stylesheet_link_tag "site", 'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css','https://fonts.googleapis.com/css?family=Nunito:200|Roboto', 'http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css' %>
